<form method="get" action="/quizes">
	<table>
		<tr>
			<td>
				<% var i=0; for(i=0;i < quizes.length ;i++) {%>
					<tr>
						<td> 
							<a href="quizes/ <%= quizes[i].id %>"><%= quizes[i].pregunta %></a>
						</td>
						<% if (i===0) {%>
						<td> 
							<input type="text" name="search" values="" />
							<input type="submit" value="Enviar" />
						</td> 	
						<% }%>

					</tr>
				<% }%>
				<% if (quizes.length===0) {%>
						<input type="text" name="search" values="" />
						<input type="submit" value="Enviar" />
				<% }%>
			</td>
		</tr>		
	</table>
</form>		